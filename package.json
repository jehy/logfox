{
  "name": "logfox",
  "version": "0.2.14",
  "description": "Application logger for express+elastic search",
  "main": "Log.js",
  "scripts": {
    "test": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "lint": "node ./node_modules/eslint/bin/eslint.js src src/test test Log.js",
    "compile": "node ./node_modules/babel-cli/bin/babel.js --presets es2015 -d ./lib src",
    "coverage": "node ./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha test/test.js",
    "prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jehy/logfox.git"
  },
  "keywords": [
    "logger",
    "log"
  ],
  "author": "Jehy",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.4.7",
    "colors": "^1.1.2",
    "fs": "0.0.1-security",
    "json-stringify-safe": "^5.0.1",
    "fs-promise": "^1.0.0",
    "object-assign": "^4.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "coveralls": "^2.12.0",
    "eslint": "^3.16.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-promise": "^3.4.2",
    "eslint-plugin-react": "^6.10.0",
    "eslint-plugin-standard": "^2.0.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0"
  }
}
